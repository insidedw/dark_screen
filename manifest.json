{
  "manifest_version": 3,
  "name": "Dark View",
  "description": "make website screen dark to protect your eyes",
  "version": "1.0.2",
  "icons": {
    "16": "images/icon-96x96.png",
    "32": "images/icon-96x96.png",
    "48": "images/icon-96x96.png",
    "96": "images/icon-96x96.png",
    "128": "images/icon-96x96.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ]
}