{
  "manifest_version": 3,
  "name": "Gray Cover",
  "description": "make website screen gray to protect your eyes",
  "version": "0.0.1",
  "icons": {
    "16": "images/icon-96x96.png",
    "32": "images/icon-96x96.png",
    "48": "images/icon-96x96.png",
    "96": "images/icon-96x96.png",
    "128": "images/icon-96x96.png"
  },
  "action": {
    "default_icon": "images/icon-96x96.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "commands": {
    "on-off": {
      "suggested_key": {
        "default": "Ctrl+Shift+0",
        "mac": "Command+Shift+0"
      },
      "description": "toggle dark-view extension",
      "global": true
    }
  }
}